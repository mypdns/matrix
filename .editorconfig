; Check http://editorconfig.org/ for more information's

root = true

[*]
trim_trailing_whitespace = true
end_of_line = LF
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4
tab_width = 4

[*.md]
trim_trailing_whitespace = false

[*.{yaml,yml}]
indent_size = 2
tab_width = 2
